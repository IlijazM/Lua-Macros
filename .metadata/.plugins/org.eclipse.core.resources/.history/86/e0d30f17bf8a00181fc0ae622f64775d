package task;

class Wait extends Task {

	@Override
	public String name() {
		return "Wait";
	}

	@Override
	public void execute(String[] args, boolean fromKey) {
		int t = 10;
		
		if (args.length >= 1) {
			if (args[0] == "Long") {
				t = 100;
			} else {
				try {
					t = Integer.parseInt(args[0]);
				} catch (NumberFormatException e) {
					// TODO: throw warn in extra console
				}
			}
		}
		
		try {
			Thread.sleep(i);
		} catch (InterruptedException e) {
			e.printStackTrace();
		}
	}

}
