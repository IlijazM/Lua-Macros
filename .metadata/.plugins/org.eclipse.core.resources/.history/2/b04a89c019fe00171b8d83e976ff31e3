package macro;

import java.util.ArrayList;

public class Macro {
	
	public static ArrayList<String> macroPaths;
	
	// print
	
	public static void Log(Object ... messages) {
		for (int i = 0; i < messages.length; i++) {
			System.out.print(String.valueOf(messages[i]) + (i != 0 ? ", " : ""));
		}
		
		System.out.println("");
	}
	
}
